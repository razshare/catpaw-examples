{
  "name": "catpaw/starter",
  "type": "project",
  "description": "A php CatPaw starter template",
  "homepage": "https://github.com/tncrazvan/catpaw-starter",
  "license": "MIT",
  "keywords": [
    "php",
    "cli",
    "catpaw",
    "examples"
  ],
  "scripts": {
    "load": "@composer dump-autoload -o",
    "script": "@php ./vendor/catpaw/core/scripts/start.php",
    
    "start:hello-world": "@script src/hello-world/main.php",
    "dev:hello-world": "@start:hello-world dev 100 && @dev:hello-world",
    "start:modules": "@script src/modules/main.php",
    "dev:modules": "@start:modules dev 100 && @dev:modules",
    "start:svelte": "@script src/modules/main.php",
    "dev:svelte": "@start:modules dev 100 && @dev:modules",
    
    "start:web:example:hello-world": "@script src/web/hello-world/main.php",
    "dev:web:example:hello-world": "@start:web:example:hello-world dev 100 && @dev:web:example:hello-world",
    "start:web:example:cats": "@script src/web/cats/main.php",
    "dev:web:example:cats": "@start:web:example:cats dev 100 && @dev:web:example:cats",
    "start:web:example:params": "@script src/web/params/main.php",
    "dev:web:example:params": "@start:web:example:params dev 100 && @dev:web:example:params",
    "start:web:example:not-found": "@script src/web/not-found/main.php",
    "dev:web:example:not-found": "@start:web:example:not-found dev 100 && @dev:web:example:not-found",
    "start:web:example:session": "@script src/web/session/main.php",
    "dev:web:example:session": "@start:web:example:session dev 100 && @dev:web:example:session",
    "start:web:example:modules": "@script src/web/modules/main.php",
    "dev:web:example:modules": "@start:web:example:modules dev 100 && @dev:web:example:modules",
    "start:web:example:web-socket": "@script src/web/web-socket/main.php",
    "dev:web:example:web-socket": "@start:web:example:web-socket dev 100 && @dev:web:example:web-socket",
    "start:web:example:byte-range": "@script src/web/byte-range/main.php",
    "dev:web:example:byte-range": "@start:web:example:byte-range dev 100 && @dev:web:example:byte-range",
    "start:web:example:custom-attributes": "@script src/web/custom-attributes/main.php",
    "dev:web:example:custom-attributes": "@start:web:example:custom-attributes dev 100 && @dev:web:example:custom-attributes",
    "start:web:example:filters": "@script src/web/filters/main.php",
    "dev:web:example:filters": "@start:web:example:filters dev 100 && @dev:web:example:filters",

    "start:example:mysql": "@script src/mysql/main.php",
    "dev:example:mysql": "@start:example:mysql dev 100 && @dev:example:mysql",

    "start:example:raspberrypi": "@script src/raspberrypi/main.php",
    "dev:example:raspberrypi": "@start:example:raspberrypi dev 100 && @dev:example:raspberrypi",

    "start:example:svelte": "@script src/svelte/main.php",
    "dev:example:svelte": "@start:example:svelte dev 100 && @dev:example:svelte"
  },
  "authors": [
    {
      "name": "Razvan Tanase",
      "email": "tangent.jotey@gmail.com",
      "homepage": "https://github.com/tncrazvan",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "catpaw/core": "^1.2",
    "catpaw/web": "^1.1",
    "catpaw/mysql": "^1.1",
    "catpaw/raspberrypi": "^1.2",
    "catpaw/svelte": "^0.1"
  },
  "require-dev": {
    "amphp/phpunit-util": "^2.0",
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "preferred-install": "dist",
    "process-timeout": 0
  }
}
