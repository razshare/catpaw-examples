{
    "name": "catpaw/examples",
    "type": "project",
    "description": "A php CatPaw starter template",
    "homepage": "https://github.com/tncrazvan/catpaw-examples",
    "license": "MIT",
    "keywords": [
        "php",
        "cli",
        "catpaw",
        "examples"
    ],
    "scripts": {
        "load": "composer dump-autoload -o",
        "fix": [
            "php -dxdebug.mode=off -dxdebug.start_with_request=no ./vendor/bin/php-cs-fixer fix src",
            "php -dxdebug.mode=off -dxdebug.start_with_request=no ./vendor/bin/php-cs-fixer fix tests"
        ],
        "action:test": [
            "php -dxdebug.mode=off -dxdebug.start_with_request=no vendor/bin/phpunit ./tests"
        ],
        "test": [
            "php -dxdebug.mode=debug -dxdebug.start_with_request=yes vendor/bin/phpunit ./tests"
        ],
        "start": "php -dxdebug.mode=debug,develop -dxdebug.start_with_request=yes ./bin/start --entry",
        "start:helloworld": "@start ./src/helloworld/main.php",
        "start:mysql": "@start ./src/mysql/main.php",
        "start:raspberrypi": "@start ./src/raspberrypi/main.php",
        "start:stores:readable": "@start ./src/stores/readable/main.php",
        "start:stores:writable": "@start ./src/stores/writable/main.php",
        "start:web:helloworld": "@start ./src/web/helloworld/main.php",
        "start:web:cats": "@start ./src/web/cats/main.php",
        "start:web:params": "@start ./src/web/params/main.php",
        "start:web:notfound": "@start ./src/web/notfound/main.php",
        "start:web:openapi": "@start ./src/web/openapi/main.php",
        "start:web:session": "@start ./src/web/session/main.php",
        "start:web:websocket": "@start ./src/web/websocket/main.php",
        "start:web:byterange": "@start ./src/web/byterange/main.php",
        "start:web:customattributes": "@start ./src/web/customattributes/main.php",
        "start:web:filters": "@start ./src/web/filters/main.php",
        "build": "php -dxdebug.mode=off -dxdebug.start_with_request=no -dphar.readonly=0 ./product.phar --build-config --build",
        "edit:extensions": "@download:product && php product.phar --extensions",
        "download:product": "[ ! -f product.phar ] && wget https://github.com/tncrazvan/catpaw-dev-tools/releases/latest/download/product.phar || echo \"product.phar found.\n\"",
        "download:psalm": "[ ! -f vendor/bin/psalm ] && wget -O vendor/bin/psalm https://github.com/vimeo/psalm/releases/latest/download/psalm.phar || echo \"vendor/bin/psalm found.\n\"",
        "post-autoload-dump": [
            "@download:product",
            "@download:psalm"
        ],
        "post-create-project-cmd": [
            "@download:product",
            "@download:psalm"
        ]
    },
    "authors": [
        {
            "name": "Razvan Tanase",
            "email": "tangent.jotey@gmail.com",
            "homepage": "https://github.com/tncrazvan",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.2",
        "catpaw/core": "^0.7",
        "catpaw/web": "^0.7",
        "catpaw/mysql": "^0.7",
        "catpaw/raspberrypi": "^0.7",
        "catpaw/store": "^0.7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpunit/phpunit": "^9"
    },
    "config": {
        "preferred-install": "dist",
        "process-timeout": 0
    }
}