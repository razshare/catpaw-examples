{
  "name": "catpaw/starter",
  "type": "project",
  "description": "A php CatPaw starter template",
  "homepage": "https://github.com/tncrazvan/catpaw-starter",
  "license": "MIT",
  "keywords": [
    "php",
    "cli",
    "catpaw",
    "examples"
  ],
  "scripts": {
    "load": "@composer dump-autoload -o",
    "script": "@php ./vendor/catpaw/core/scripts/start.php",
    "start:helloworld": "@script src/helloworld/main.php",
    "dev:helloworld": "@start:helloworld dev 100 && @dev:helloworld",
    "start:modules": "@script src/modules/main.php",
    "dev:modules": "@start:modules dev 100 && @dev:modules",
    "start:web:helloworld": "@script src/web/helloworld/main.php",
    "dev:web:helloworld": "@start:web:helloworld dev 100 && @dev:web:helloworld",
    "start:web:cats": "@script src/web/cats/main.php",
    "dev:web:cats": "@start:web:cats dev 100 && @dev:web:cats",
    "start:web:params": "@script src/web/params/main.php",
    "dev:web:params": "@start:web:params dev 100 && @dev:web:params",
    "start:web:notfound": "@script src/web/notfound/main.php",
    "dev:web:notfound": "@start:web:notfound dev 100 && @dev:web:notfound",
    "start:web:session": "@script src/web/session/main.php",
    "dev:web:session": "@start:web:session dev 100 && @dev:web:session",
    "start:web:modules": "@script src/web/modules/main.php",
    "dev:web:modules": "@start:web:modules dev 100 && @dev:web:modules",
    "start:web:web-socket": "@script src/web/web-socket/main.php",
    "dev:web:web-socket": "@start:web:web-socket dev 100 && @dev:web:web-socket",
    "start:web:byterange": "@script src/web/byterange/main.php",
    "dev:web:byterange": "@start:web:byterange dev 100 && @dev:web:byterange",
    "start:web:customattributes": "@script src/web/customattributes/main.php",
    "dev:web:customattributes": "@start:web:customattributes dev 100 && @dev:web:customattributes",
    "start:web:filters": "@script src/web/filters/main.php",
    "dev:web:filters": "@start:web:filters dev 100 && @dev:web:filters",
    "start:mysql": "@script src/mysql/main.php",
    "dev:mysql": "@start:mysql dev 100 && @dev:mysql",
    "start:raspberrypi": "@script src/raspberrypi/main.php",
    "dev:raspberrypi": "@start:raspberrypi dev 100 && @dev:raspberrypi",
    "start:web:svelte": "@script src/web/svelte/main.php",
    "dev:web:svelte": "@start:web:svelte dev 100 && @dev:web:svelte"
  },
  "authors": [
    {
      "name": "Razvan Tanase",
      "email": "tangent.jotey@gmail.com",
      "homepage": "https://github.com/tncrazvan",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "catpaw/core": "^1.2",
    "catpaw/web": "^1.1",
    "catpaw/mysql": "^1.1",
    "catpaw/raspberrypi": "^1.2",
    "catpaw/svelte": "^0.1",
    "ext-pcntl": "*"
  },
  "require-dev": {
    "amphp/phpunit-util": "^2.0",
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "preferred-install": "dist",
    "process-timeout": 0
  }
}
