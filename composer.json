{
  "name": "catpaw/starter",
  "type": "project",
  "description": "A php CatPaw starter template",
  "homepage": "https://github.com/tncrazvan/catpaw-starter",
  "license": "MIT",
  "keywords": [
    "php",
    "cli",
    "catpaw",
    "examples"
  ],
  "scripts": {
    "load": "@composer dump-autoload -o",
    "script": "@php ./vendor/catpaw/core/scripts/start.php",
    "fix": [
      "@php ./vendor/bin/php-cs-fixer fix src",
      "@php ./vendor/bin/php-cs-fixer fix tests"
    ],
    "start:helloworld": "@script src/helloworld/main.php",
    "dev:helloworld": "composer start:helloworld dev 100 && composer dev:helloworld",
    "start:modules": "@script src/modules/main.php",
    "dev:modules": "composer start:modules dev 100 && composer dev:modules",
    "start:web:helloworld": "@script src/web/helloworld/main.php",
    "dev:web:helloworld": "composer start:web:helloworld dev 100 && composer dev:web:helloworld",
    "start:web:cats": "@script src/web/cats/main.php",
    "dev:web:cats": "composer start:web:cats dev 100 && composer dev:web:cats",
    "start:web:params": "@script src/web/params/main.php",
    "dev:web:params": "composer start:web:params dev 100 && composer dev:web:params",
    "start:web:notfound": "@script src/web/notfound/main.php",
    "dev:web:notfound": "composer start:web:notfound dev 100 && composer dev:web:notfound",
    "start:web:session": "@script src/web/session/main.php",
    "dev:web:session": "composer start:web:session dev 100 && composer dev:web:session",
    "start:web:modules": "@script src/web/modules/main.php",
    "dev:web:modules": "composer start:web:modules dev 100 && composer dev:web:modules",
    "start:web:web-socket": "@script src/web/web-socket/main.php",
    "dev:web:web-socket": "composer start:web:web-socket dev 100 && composer dev:web:web-socket",
    "start:web:byterange": "@script src/web/byterange/main.php",
    "dev:web:byterange": "composer start:web:byterange dev 100 && composer dev:web:byterange",
    "start:web:customattributes": "@script src/web/customattributes/main.php",
    "dev:web:customattributes": "composer start:web:customattributes dev 100 && composer dev:web:customattributes",
    "start:web:filters": "@script src/web/filters/main.php",
    "dev:web:filters": "composer start:web:filters dev 100 && composer dev:web:filters",
    "start:mysql": "@script src/mysql/main.php",
    "dev:mysql": "composer start:mysql dev 100 && composer dev:mysql",
    "start:raspberrypi": "@script src/raspberrypi/main.php",
    "dev:raspberrypi": "composer start:raspberrypi dev 100 && composer dev:raspberrypi"
  },
  "authors": [
    {
      "name": "Razvan Tanase",
      "email": "tangent.jotey@gmail.com",
      "homepage": "https://github.com/tncrazvan",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "catpaw/core": "^1.2",
    "catpaw/web": "^1.1",
    "catpaw/mysql": "^1.1",
    "catpaw/raspberrypi": "^1.2",
    "catpaw/store": "^1.0",
    "catpaw/openapi": "^1.0"
  },
  "require-dev":{
    "phpunit/phpunit": "^9.5",
    "friendsofphp/php-cs-fixer": "^3.8"
  },
  "config": {
    "preferred-install": "dist",
    "process-timeout": 0
  }
}
